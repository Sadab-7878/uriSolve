#include <stdio.h>
#include <stdlib.h>

int main()
{
   char str[100];
   int n=0,a;
   scanf("%d",&n);
    int num=0;

   for(a=1;a<=n;a++){

        printf("Enter a String: ");
        fflush(stdin);
        gets(str);
        int sum=0;
        for(int i=0;i<14;i++)
        {
        if((str[i]>='0')&& (str[i]<='9'))
       {
          num = num*10 + (str[i]-'0');



       }
       else{
        sum += num;
        num=0;
       }








   }
 printf("%d\n",sum);



   }



    return 0;
}
